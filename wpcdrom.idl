    local;
    majorversion = 1;
    minorversion = 2;
    filestem = wpcdrom;
    callstyle = oidl;
    dllname = "pnp.dll";


    /* Overridden class methods section */
    wpclsInitData:      override;
    wpclsQueryIconData: override;
    wpclsQueryTitle:    override;
    wpclsUnInitData:    override;
  };
};
#endif  /* wpcdrom_idl */
